{
  "source": "en-US",
  "targets": [
    "zh-CN",
    "ja-JP",
    "ko-KR",
    "es-ES",
    "fr-FR",
    "de-DE"
  ],
  "include": [
    "README_EN.md",
    "packages/ui/src/i18n/locales/en-US.ts",
